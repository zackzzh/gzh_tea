html {
  background: #f4f4f4;
  align-items: center;
  flex: 1;
  min-height: calc(100vh);
}
html [v-cloak] {
  display: none;
}
html .main_div {
  padding-bottom: .666667rem;
}
html .main_div .main_div_main {
  background: #fff;
  padding-bottom: .666667rem;
}
html .main_div .title {
  padding: .533333rem;
  font-size: .426667rem;
}
html .main_div .title_div {
  width: 6.666667rem;
  text-align: center;
  padding: .8rem;
  margin: 0 auto;
}
html .main_div .main_div_button {
  position: fixed;
  bottom: .533333rem;
  width: 100%;
  display: flex;
  justify-content: center;
}
html .main_div .van-button {
  margin-top: .533333rem;
  width: 95%;
}
html .main_div .dialog_row {
  margin: .666667rem;
}
html .main_div .dialog_row .dialog_radio {
  display: flex;
  justify-content: center;
}
