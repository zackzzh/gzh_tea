.box_type_div {
    margin-bottom: 1.173333rem;
    padding      : .346667rem 0;

    .fourStep-detail {
        background   : rgba(255, 255, 255, 1);
        box-shadow   : 0px 0px 35px 0px rgba(18, 46, 116, 0.1);
        border-radius: .133333rem;
        margin       : 0 .32rem;
        margin-top   : .426667rem;
        color        : #9E918F;

        .box_type_cell_right_icon {
            margin-left: .293333rem;
            color      : #007D7B;
        }


        .commodity_card {
            margin-top      : 0;
            padding         : .346667rem .266667rem;
            border-radius   : .133333rem;
            background      : #FFFFFF;
            // box-shadow   : 0 0 .466667rem 0 rgba(18, 46, 116, 0.1);
            border-bottom   : 1px solid #f7f7f7;

            .van-card__num {
                text-align: right;
            }

            .van-card__title {
                font-family: PingFang SC;
                font-size  : .373333rem;
                font-weight: 500;
                color      : #000000;
            }

            .commodity_img {
                border: 1px solid #EEEEEE;
            }

            .commodity_price_fun() {
                margin-top: .533333rem;
            }

            .commodity_price {
                .commodity_price_fun;
                font-size  : .32rem;
                color      : #007D7B;
                font-weight: 500;
            }

            .commodity_totalPrices {
                .commodity_price_fun;
                font-family: PingFang SC;
                font-size  : .426667rem;
                font-weight: 500;
                color      : #007D7B;
            }

            .commodity_number {
                font-size: .32rem;
                color    : #007D7B;
            }
        }
    }

    .cell_address_title {
        font-family: PingFang SC;
        font-size  : .373333rem;
        font-weight: 500;
        color      : #B2B2B2;
    }

   

    .fourStep-button {
        display        : flex;
        align-items    : center;
        justify-content: space-between;
        position       : fixed;
        bottom         : 0;
        width          : 100%;

        .button-left {
            width          : 5.973333rem;
            height         : 1.173333rem;
            line-height    : 1.066667rem;
            background     : rgba(116, 182, 181, 1);
            font-size      : .4rem;
            text-align     : center;
            display        : flex;
            align-items    : baseline;
            justify-content: center;
            font-family    : PingFang SC;
            font-weight    : 500;
            color          : #fff;

            .span-one {
                font-size: .453333rem;
                margin   : 0px 5px;
                color    : #007D7B;
            }

            .span-two {
                font-size: .213333rem;
                color    : #007D7B;
            }
        }

        .button-right {
            width      : 4.026667rem;
            height     : 1.173333rem;
            line-height: 1.066667rem;
            text-align : center;
            background : rgba(0, 125, 123, 1);
            font-size  : .4rem;
            font-family: PingFang SC;
            font-weight: 500;
            color      : rgba(255, 255, 255, 1);
        }
    }
}