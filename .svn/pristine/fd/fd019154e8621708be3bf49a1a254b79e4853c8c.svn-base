[v-cloak] {
  display: none;
}
.main_div {
  padding-bottom: .48rem;
}
.main_div .search_bar_row {
  padding: 0 .533333rem;
}
.main_div .search_bar_row .search_bar_left {
  text-align: center;
}
.main_div .search_bar_row .search_bar_left span {
  font-size: .533333rem;
  font-weight: bold;
}
.main_div .search_bar_row .search_bar_input {
  padding: .266667rem 0 .266667rem .266667rem;
}
.main_div .filtrate_row {
  margin: .133333rem 0 .4rem 0;
  text-align: center;
}
.main_div .filtrate_row .filtrate_title {
  font-family: Source Han Sans CN;
  font-size: .32rem;
  font-weight: 400;
  color: #333333;
}
.main_div .filtrate_row .filtrate_title .filtrate_title_text {
  width: auto;
  margin: 0 25%;
  padding-bottom: .133333rem;
}
.main_div .filtrate_row .filtrate_title .filtrate_title_text span {
  margin-left: .133333rem;
  font-size: .32rem;
}
.main_div .filtrate_row .filtrate_title_active {
  color: #F95043;
}
.main_div .custom_popup {
  width: 80%;
  height: 100%;
  padding: 0 .4rem;
}
.main_div .custom_popup .popup_icon {
  margin: .4rem 0 .586667rem 0;
}
.main_div .custom_popup .popup-button {
  position: fixed;
  bottom: 0px;
  display: flex;
  align-items: center;
}
.main_div .custom_popup .popup-button .van-button {
  padding: 0px;
  height: 1.066667rem;
  line-height: 1.066667rem;
  width: 3.733333rem;
  margin: 0 0.133333rem;
}
.main_div .custom_popup .van-cell-group {
  display: flex;
  align-items: center;
}
.main_div .custom_popup .van-field__control {
  border: 1px solid #e5e5e5;
  border-radius: 10px;
  text-align: center;
}
.main_div .custom_popup .popup_main_div .popup_title {
  font-family: PingFang SC;
  font-size: .4rem;
  font-weight: 500;
  color: #222222;
  letter-spacing: .026667rem;
}
.main_div .custom_popup .popup_main_div .popup_row {
  margin: .4rem 0 .533333rem 0;
}
.main_div .custom_popup .popup_main_div .popup_row .popup_btn_item {
  height: auto;
  margin-right: .266667rem;
  padding: .24rem .32rem;
  border-radius: .133333rem;
  margin-bottom: .266667rem;
  font-family: PingFang SC;
  font-size: .32rem;
  font-weight: 500;
  color: #222222;
  line-height: .32rem;
  background: #F6F6F6;
}
.main_div .custom_popup .popup_main_div .popup_row .first_active,
.main_div .custom_popup .popup_main_div .popup_row .second_active,
.main_div .custom_popup .popup_main_div .popup_row .third_active {
  border: 1px solid #006765;
  color: #006765;
}
.main_div .van-image__img {
  width: 4.56rem;
  height: 4.56rem;
}
.main_div .card_row {
  margin-top: .293333rem;
  padding: 0 .08rem;
}
.main_div .card_row .card_col {
  margin-bottom: .266667rem;
  padding: 0 .186667rem;
}
.main_div .card_row .card_col .card_div {
  border: 1px solid rgba(0, 0, 0, 0.15);
  border-radius: .133333rem;
  overflow: hidden;
}
.main_div .card_row .card_col .card_div .card_title {
  padding: .293333rem 0 0 .266667rem;
  font-family: PingFang SC;
  font-size: .4rem;
  font-weight: 500;
  text-align: center;
  width: 90%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.main_div .card_row .card_col .card_div .card_title_row {
  padding: .293333rem 0 0 .266667rem;
}
.main_div .card_row .card_col .card_div .card_title_row .card_title_col div {
  font-family: PingFang SC;
  font-size: .213333rem;
  font-weight: 500;
  color: rgba(0, 103, 101, 0.62);
}
.main_div .card_row .card_col .card_div .price_row {
  margin: .346667rem 0 .266667rem 0;
  padding: 0 .186667rem 0 .266667rem;
}
.main_div .card_row .card_col .card_div .price_row .price_col {
  font-family: PingFang SC;
  font-size: 12px;
  font-weight: 500;
  color: #006765;
}
.main_div .card_row .card_col .card_div .price_row .price_col span {
  margin-left: .133333rem;
  font-size: .32rem;
}
.main_div .card_row .card_col .card_div .price_row .price_btn_col .price_btn {
  margin: 0;
  padding: .106667rem .16rem;
  height: auto;
  outline: none;
  border: none;
  border-radius: .133333rem;
  line-height: .506667rem;
  font-family: PingFang SC;
  font-size: .32rem;
  font-weight: 500;
  background: #007D7B;
}
