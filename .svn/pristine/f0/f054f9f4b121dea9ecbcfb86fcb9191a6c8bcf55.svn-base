body {
  background: #f7f7f7;
  flex: 1;
  align-items: center;
  min-height: calc(100vh);
}
body [v-cloak] {
  display: none;
}
body .main_info_div {
  padding: 0px .266667rem;
  padding-bottom: 1.333333rem;
}
body .main_info_div .info-detail {
  background: #ffffff;
  box-shadow: 0px 0px 35px 0px rgba(18, 46, 116, 0.1);
  border-radius: .133333rem;
  margin-top: .266667rem;
  margin-bottom: .266667rem;
  font-size: .346667rem;
  padding-bottom: .266667rem;
}
body .main_info_div .info-detail .detail-item {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e5e5e5;
  padding: 0 .533333rem;
}
body .main_info_div .info-detail .detail-item .item-label {
  width: 2rem;
  color: #4C4C4C;
  font-weight: 500;
}
body .main_info_div .info-detail .detail-item .item-select {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  padding-top: .266667rem;
}
body .main_info_div .info-detail .detail-item .item-select .item-span {
  display: inline-block;
  height: .746667rem;
  line-height: .746667rem;
  border-radius: 10px;
  padding: 0 .4rem;
  margin-right: .266667rem;
  margin-bottom: .266667rem;
  border: 1px solid rgba(0, 0, 0, 0.4);
  color: rgba(0, 0, 0, 0.4);
}
body .main_info_div .info-detail .detail-item .item-select .box_types_active,
body .main_info_div .info-detail .detail-item .item-select .material_qualitys_active,
body .main_info_div .info-detail .detail-item .item-select .technologys_active,
body .main_info_div .info-detail .detail-item .item-select .surfaces_active,
body .main_info_div .info-detail .detail-item .item-select .nettos_active,
body .main_info_div .info-detail .detail-item .item-select .lining_papers_active,
body .main_info_div .info-detail .detail-item .item-select .outer_boxs_active {
  border: 1px solid #007D7B;
  color: #007D7B;
}
body .main_info_div .info-detail .detail-item .item-select .types_default {
  color: #D1C0C4;
  border: 1px solid #DDDDDD;
}
body .main_info_div .info-detail .detail-list .list-bottom .van-cell__value {
  color: #4C4C4C;
}
body .main_info_div .info-detail .detail-list .list-bottom-ths {
  text-decoration: line-through;
}
body .main_info_div .info-detail .detail-list .van-cell__value {
  color: #007D7B;
  font-size: .506667rem;
}
body .main_info_div .info-detail .detail-list .van-stepper .van-stepper__minus,
body .main_info_div .info-detail .detail-list .van-stepper .van-stepper__plus {
  border: 1px solid #f2f3f5;
  background: transparent;
}
body .main_info_div .info-detail .detail-list .van-stepper .van-stepper__input {
  border: 1px solid #f2f3f5;
  background: transparent;
  width: 1.2rem;
  margin: 0px;
  border-left: none;
  border-right: none;
}
body .main_info_div .info-tech {
  padding: .4rem 0;
}
body .main_info_div .info-tech .tech-title {
  width: 2.133333rem;
  margin: 0 auto;
  color: #007D7B;
  font-size: .453333rem;
  text-align: center;
}
body .main_info_div .info-tech .tech-line {
  width: 2.133333rem;
  height: .053333rem;
  background: #007D7B;
  margin: .266667rem auto;
}
body .main_info_div .info-tech .tech-scroll {
  width: 100%;
}
body .main_info_div .info-tech .tech-scroll .tech-item {
  display: flex;
  overflow-x: auto;
}
body .main_info_div .info-tech .tech-scroll .item-one {
  max-width: 40%;
  flex: 1 40%;
  flex-shrink: 0;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content {
  width: 100%;
  background: #74B6B5;
  box-shadow: 0px 0px 10px 0px rgba(0, 46, 30, 0.63);
  margin-top: -0.08rem;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .line-title {
  width: 2.533333rem;
  text-align: center;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #007D7B;
  font-size: .293333rem;
  padding: .133333rem 0;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .van-stepper {
  display: flex;
  justify-content: center;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .van-stepper .van-stepper__minus,
body .main_info_div .info-tech .tech-scroll .item-one .line-content .van-stepper .van-stepper__plus {
  border: 1px solid #007D7B;
  color: #007D7B;
  background: transparent;
  height: .666667rem;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .van-stepper .van-stepper__input {
  height: .666667rem;
  width: 1.2rem;
  background: transparent;
  border: 1px solid #007D7B;
  color: #007D7B;
  margin: 0px;
  border-left: none;
  border-right: none;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .line-bottom {
  padding: .266667rem;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .line-bottom .van-checkbox__icon--checked .van-icon {
  background: #007D7B;
  border-color: #007D7B;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .line-bottom .van-checkbox__label {
  color: #007D7B;
}
body .main_info_div .info-tech .tech-scroll .item-one .line-content .line-bottom a {
  color: #007D7B;
}
body .info-bottom .wechat {
  color: #006C69;
  background-color: #74B6B5;
}
body .info-bottom .wechat a {
  color: #006C69;
}
body .info-bottom .wechat .van-goods-action-icon__icon {
  color: #006C69;
}
body .info-bottom .van-button--warning {
  background: #007d7b;
  border: none;
}
