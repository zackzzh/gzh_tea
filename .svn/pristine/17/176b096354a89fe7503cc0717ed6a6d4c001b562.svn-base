body {
  background: #f7f7f7;
  flex: 1;
  align-items: center;
  min-height: calc(100vh);
}
body [v-cloak] {
  display: none;
}
body .main_div .header_div {
  background-image: url("../../img/mine/bg.png");
  background-repeat: no-repeat;
  background-size: 100% auto;
}
body .main_div .header_div .nickname_row {
  padding: .8rem 0 .666667rem .32rem;
  font-family: Adobe Heiti Std;
  color: #FFFEFE;
}
body .main_div .header_div .nickname_row .nickname_col_img {
  margin-right: .533333rem;
}
body .main_div .header_div .nickname_row .nickname_title {
  font-size: .426667rem;
  letter-spacing: .053333rem;
}
body .main_div .header_div .nickname_row .nickname_phone {
  font-size: .4rem;
  letter-spacing: .013333rem;
}
body .main_div .header_div .items_row {
  margin: 0 .32rem;
  padding: 0 .266667rem;
  border-radius: .08rem;
  background: #FFFFFF;
  text-align: center;
}
body .main_div .header_div .items_row .item_col {
  padding: .8rem .32rem 0 .32rem;
}
body .main_div .header_div .items_row .item_col .item_title {
  margin: .4rem 0 .8rem 0;
  font-family: Microsoft YaHei;
  font-size: .346667rem;
  font-weight: 400;
  color: #009D8A;
}
body .main_div .app_div {
  margin: .32rem .32rem 1.6rem .32rem;
  padding-top: .4rem;
  border-radius: .08rem;
  background: #FFFFFF;
}
body .main_div .app_div .app_mian_title {
  padding-left: .213333rem;
  border-left: .106667rem solid #6D56FF;
  font-size: .426667rem;
  color: #333333;
  letter-spacing: .026667rem;
}
body .main_div .app_div .app_row {
  margin-top: .133333rem;
  padding: 0 .133333rem;
  font-family: Microsoft YaHei;
  font-size: .373333rem;
  font-weight: 400;
  text-align: center;
  color: #666666;
}
body .main_div .app_div .app_row .app_col {
  padding: .266667rem 0;
  border-bottom: 1px solid #E7E8ED;
  border-right: 1px solid #E7E8ED;
  font-size: 12px;
}
body .main_div .app_div .app_row .app_col:nth-child(3),
body .main_div .app_div .app_row .app_col:nth-child(6) {
  border-right: none;
}
body .main_div .app_div .app_row .app_col:nth-child(4),
body .main_div .app_div .app_row .app_col:nth-child(5),
body .main_div .app_div .app_row .app_col:nth-child(6) {
  border-bottom: none;
}
body .main_div .app_div .app_row .app_col .app_title {
  margin: .4rem 0 .346667rem 0;
}
