body {
  background: #f7f7f7;
  flex: 1;
  align-items: center;
  min-height: calc(100vh);
}
body [v-cloak] {
  display: none;
}
body .box_type_step_div {
  padding: 0 .666667rem;
  padding-top: .373333rem;
}
body .box_type_step_div .box_type_step_img_div {
  margin-bottom: .32rem;
  padding: 0 .266667rem;
}
body .box_type_step_div .box_type_step_row {
  margin-left: .133333rem;
  text-align: center;
}
body .box_type_step_div .box_type_step_row .box_type_step_col {
  font-family: Source Han Sans CN;
  font-size: .293333rem;
  font-weight: 400;
  color: #010101;
  line-height: .426667rem;
  opacity: 0.6;
}
