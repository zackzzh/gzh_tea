[v-cloak] {
  display: none;
}
.main_div {
  padding-bottom: 1.6rem;
}
.main_div .banner_div {
  position: relative;
}
.main_div .banner_div .search_bar_row {
  width: 100%;
}
.main_div .banner_div .search_bar_row .search_bar_left {
  text-align: center;
}
.main_div .banner_div .search_bar_row .search_bar_left span {
  font-size: .533333rem;
  font-weight: bold;
}
.main_div .banner_div .search_bar_row .van-search__content {
  background: rgba(247, 247, 247, 0.8);
}
.main_div .index_item_row {
  text-align: center;
  margin-top: .346667rem;
}
.main_div .index_item_row .index_item_col {
  padding: 0 .4rem;
}
.main_div .index_item_row .index_item_col .item_title {
  margin: .32rem 0;
  font-size: .32rem;
  color: #333333;
}
.main_div .index_line {
  width: 100%;
  height: 20px;
  background: #f7f7f7;
}
.main_div .more_news {
  width: 100%;
  text-align: end;
  margin: .106667rem 0 .186667rem 0;
}
.main_div .more_news span {
  padding-right: .506667rem;
  font-size: .24rem;
}
.main_div .news_row {
  padding: 0 .4rem;
}
.main_div .news_row .news_col_img {
  padding-right: .24rem;
}
.main_div .news_row .news_items_col {
  border-left: 1px solid #E5E5E5;
}
.main_div .news_row .news_items_col .news_item_row {
  margin-bottom: .133333rem;
  padding-left: .24rem;
}
.main_div .news_row .news_items_col .news_item_row .news_item_col {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.main_div .newbie_row {
  margin-top: .453333rem;
  padding: 0 .533333rem;
}
.main_div .newbie_row .newbie_left {
  padding-right: 0.08rem;
}
.main_div .newbie_row .newbie_right {
  padding-left: 0.08rem;
}
.main_div .main_div_top {
  text-align: center;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: .373333rem;
}
.main_div .recommend_title {
  margin-top: .4rem;
  text-align: center;
  font-family: PingFang SC;
  font-size: .426667rem;
  font-weight: 500;
  color: #383838;
  letter-spacing: 4;
}
.main_div .recommend_title span {
  font-size: .426667rem;
  font-weight: bold;
  color: #565656;
}
.main_div .card_row {
  margin-top: .293333rem;
  padding: 0 .08rem;
}
.main_div .card_row .card_col {
  padding: 0 .186667rem;
  margin-bottom: .266667rem;
}
.main_div .card_row .card_col .card_div {
  padding-bottom: .24rem;
  box-shadow: 0 0 0.16rem 0 rgba(0, 0, 0, 0.15);
  border-radius: .133333rem;
}
.main_div .card_row .card_col .card_div .van-image__img {
  width: 100%;
  height: 4.546667rem;
}
.main_div .card_row .card_col .card_div .card_title_row {
  padding: .293333rem 0 0 .266667rem;
}
.main_div .card_row .card_col .card_div .card_title_row .card_title_col div {
  font-family: PingFang SC;
  font-size: .213333rem;
  font-weight: 500;
  color: rgba(0, 103, 101, 0.62);
}
.main_div .card_row .card_col .card_div .price_row {
  margin: .346667rem 0 .266667rem 0;
  padding: 0 .186667rem 0 .266667rem;
}
.main_div .card_row .card_col .card_div .price_row .price_col {
  font-family: PingFang SC;
  font-size: 14px;
  font-weight: 500;
  color: #006765;
}
.main_div .card_row .card_col .card_div .price_row .price_col span {
  margin-left: .133333rem;
  font-size: .32rem;
}
.main_div .card_row .card_col .card_div .price_row .price_btn_col .price_btn {
  margin: 0;
  padding: .106667rem .16rem;
  height: auto;
  outline: none;
  border: none;
  border-radius: .133333rem;
  line-height: .506667rem;
  font-family: PingFang SC;
  font-size: .32rem;
  font-weight: 500;
  background: #007D7B;
}
.main_div .deseno_row {
  padding: .586667rem .613333rem 0 .72rem;
}
.main_div .deseno_row .deseno_time {
  padding: 0 0 0 .373333rem;
  font-family: PingFang SC;
  font-size: .32rem;
  font-weight: 500;
  color: #676767;
}
.main_div .deseno_row .deseno_more {
  font-family: PingFang SC;
  font-size: .32rem;
  font-weight: 500;
  color: #666666;
  text-align: right;
}
